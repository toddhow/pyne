		"@sapphire/fetch": "next",
		"@sapphire/framework": "^2.1.4",
		"@sapphire/plugin-api": "next",
		"@sapphire/plugin-editable-commands": "next",
		"@sapphire/plugin-logger": "next",
		"@sapphire/plugin-subcommands": "next",
		"@sapphire/snowflake": "^2.1.4",
		"@sapphire/time-utilities": "next",
		"@sapphire/type": "2.1.2",
		"@sapphire/utilities": "3.0.7",
		"@skyra/editable-commands": "^2.1.0",
		"axios": "^0.24.0",
		"colorette": "^2.0.16",
		"dayjs": "^1.10.7",
		"discord.js": "^13.3.1",
		"dotenv-cra": "^3.0.2",
		"prisma": "^3.4.2"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.0.3",
		"@sapphire/prettier-config": "^1.2.4",
		"@sapphire/ts-config": "^3.1.4",
		"@types/node": "^16.11.7",
		"@types/ws": "^8.2.0",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"discord-api-types": "^0.24.0",
		"dotenv-cli": "^4.1.0",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.4.1",
		"tsc-watch": "^4.5.0",
		"tslib": "^2.3.1",
		"typescript": "^4.4.4"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1"
	},
	"scripts": {
		"build": "run-s build:tsc",
		"build:tsc": "tsc -b src",
		"watch": "tsc -w",
		"start": "node dist/index.js",
		"dev": "run-s build start",
		"format": "prettier --write --loglevel=warn \"src/**/*.{js,ts,json}\"",
		"lint": "eslint --fix --ext ts src",
		"migrate": "dotenv -e .env.local -- prisma migrate deploy"
	},
	"engines": {
		"node": "^16",
		"yarn": "3.1.0"
	},
	"prettier": "@sapphire/prettier-config",
	"prisma": {
		"schema": "prisma/schema.prisma"
	},
	"packageManager": "yarn@3.1.0"
